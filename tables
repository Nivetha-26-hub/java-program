import java.util.Scanner;

class multiply {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.println("Enter the number:");
        int num = scan.nextInt();
        int n = 15;

        for (int i = 0; i <= n; i++)
            System.out.println(i + "x" + num + "=" + (num * i));

        scan.close();
    }
}
